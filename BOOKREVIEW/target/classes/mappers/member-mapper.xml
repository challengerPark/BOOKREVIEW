<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">

	 
	 
	 <select id="login" resultType="member">
	 	SELECT
	 		    *
	 	FROM 
	 		  MEMBER
	 	WHERE 
	 		  MEMBER_ID = #{memberId}
	 	AND
	 		  MEMBER_PWD = #{memberPwd}
	 </select>
	 
	 
	 
	 
</mapper>